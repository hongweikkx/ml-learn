   "source": [
    "import tensorflow as tf\n",
    "\n",
    "x = tf.range(4, dtype=tf.float32)\n",
    "x = tf.Variable(x)\n",
    "\n",
    "with tf.GradientTape() as t:\n",
    "    y = 2 *tf.tensordot(x, x, axes=1)\n",
    "print(y)\n",
    "x_grad = t.gradient(y, x)\n",
    "print(x_grad)"
   ],
